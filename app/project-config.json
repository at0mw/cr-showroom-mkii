{
  "projectName": "cr-showroom-mkii",
  "version": "0.0.1",
  "faviconPath": "favicon.ico",
  "menuOrientation": "none",
  "selectedTheme": "crestron-theme",
  "useWebXPanel": true,
  "themes": [
    {
      "name": "crestron-theme",
      "extends": "light-theme",
      "brandLogo": {
        "url": "./app/template/assets/img/proAV-logo-transparent.png",
        "alt": "proAV Logo",
        "receiveStateUrl": ""
