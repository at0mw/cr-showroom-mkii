        "receiveStateUrl": ""
      },
      "backgroundProperties": {
        "backgroundColor": [
          "#f8f8f8"
        ]
      }
    },
    {
      "name": "dark-theme",
      "extends": "dark-theme",
      "brandLogo": {
        "url": "./app/template/assets/img/ch5-logo-dark.svg",
        "alt": "Crestron Logo",
        "receiveStateUrl": ""
      },
      "backgroundProperties": {
        "backgroundColor": [
          "#1a1a1a"
        ]
      }
    },
    {
      "name": "project-light-theme",
      "extends": "light-theme",
